{
    "version": 2,
    "builds": [
      {
        "src": "lib/server.ts",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/_next/static/(.*)",
        "dest": "/_next/static/$1"
      },
      {
        "src": "/_next/image/(.*)",
        "dest": "/_next/image/$1"
      },
      {
        "src": "/api/claim",
        "methods": ["POST"],
        "dest": "lib/server.ts"
      },
      {
        "src": "/api/(.*)",
        "dest": "lib/server.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/$1",
        "headers": {
            "x-vercel-cache": "MISS"
        }
      }
    ]
  }